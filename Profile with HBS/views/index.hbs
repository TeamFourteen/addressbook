<!DOCTYPE html>
<html>
    <head>
        <title>New Profile Page</title>
        <link href="index.css" rel="stylesheet" type="text/css" />
    </head>

    <body>
        <div id="display">
           
            <div id="profileDiv">
                {{#each profileName}}
                    <div id="{{profileName}}" class="columnName">{{this.fname}} 
                        <br />
                        {{this.lname}}</div>
                {{/each}}
                
                
                <div id="{{bio}}" class="bio">{{bio}}</div>
            </div>
            
            <div id="contactDiv">
                <div id="contactInfo">Contact Information</div>
                {{#each phoneNumber}}
                    <div id="{{this.phone_id}}" class="phones">{{this.type}} {{this.phone}}</div>
                    <br />
                {{/each}}
                
            </div>
            
            <div id="addressDiv">
                Address
                <div id="googleMap" style="width:100%;height:0px; transition:1s;"></div>
            </div>
            
        </div>
        
        <div id="sideBar"></div>
        
        <div id="navBar"></div>
        <script>
            function myMap() {
                var mapProp= {
        center:new google.maps.LatLng(51.508742,-0.120850),
        zoom:5,
                };
                var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);
            }
        </script>

        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBIqbp_YK9jE1DezYa8WS9AFIreOz0lErA&callback=myMap"></script>
        
        <script src="index.js"></script>
    </body>

</html>